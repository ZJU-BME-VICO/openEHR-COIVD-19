definition
	EVALUATION[at0000] occurrences matches {0..1} matches {	-- Epidemiological history questionnaire about COVID_19
		data matches {
			ITEM_TREE[at0001] matches {	-- ITEM_TREE
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Travel history or residence history 
						value matches {
							0|[local::at0003], 	-- No
							1|[local::at0004]  	-- Yes
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- History of contact with the infected people of SARS-COV-2
						value matches {
							0|[local::at0006], 	-- No
							1|[local::at0007]  	-- Yes
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {	-- History of contact with patients with fever or respiratory symptoms
						value matches {
							0|[local::at0009], 	-- No
							1|[local::at0010]  	-- Yes
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Aggressive onset
						value matches {
							0|[local::at0012], 	-- No
							1|[local::at0013]  	-- Yes
						}
					}
				}
			}
		}
	}

