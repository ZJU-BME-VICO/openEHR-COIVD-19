  "definition": {
    "data_bindings": {
      "gt0002": {
        "id": "gt0002",
        "model_id": "openEHR-EHR-EVALUATION.problem_diagnosis.v1",
        "template_id": "openEHR-EHR-EVALUATION.problem_diagnosis.v1",
        "type": "INPUT",
        "elements": {
          "gt0003": {
            "id": "gt0003",
            "path": "/data[at0001]/items[at0073]"
          },
          "gt0015": {
            "id": "gt0015",
            "path": "/data[at0001]/items[at0005]"
          },
          "gt0021": {
            "id": "gt0021",
            "path": "/data[at0001]/items[at0002]"
          },
          "gt0024": {
            "id": "gt0024",
            "path": "/data[at0001]/items[openEHR-EHR-CLUSTER.problem_qualifier.v1]/items[at0083]"
          },
          "gt0027": {
            "id": "gt0027",
            "path": "/protocol[at0032]/items[at0070]"
          }
        }
      },
      "gt0008": {
        "id": "gt0008",
        "model_id": "openEHR-EHR-INSTRUCTION.medication_order.v2",
        "template_id": "openEHR-EHR-INSTRUCTION.medication_order.v2",
        "type": "OUTPUT",
        "elements": {
          "gt0009": {
            "id": "gt0009",
            "path": "/activities[at0001]/description[at0002]/items[at0070]"
          }
        }
      },
      "gt0025": {
        "id": "gt0025",
        "model_id": "openEHR-EHR-EVALUATION.problem_diagnosis.v1",
        "template_id": "openEHR-EHR-EVALUATION.problem_diagnosis.v1",
        "type": "OUTPUT",
        "elements": {
          "gt0026": {
            "id": "gt0026",
            "path": "/protocol[at0032]/items[at0070]"
          }
        }
      },
      "gt0028": {
        "id": "gt0028",
        "model_id": "openEHR-EHR-EVALUATION.recommendation.v1",
        "template_id": "openEHR-EHR-EVALUATION.recommendation.v1",
        "type": "OUTPUT",
        "elements": {
          "gt0029": {
            "id": "gt0029",
            "path": "/data[at0001]/items[at0002]"
          }
        }
      }
    },
    "rules": {
      "gt0016": {
        "id": "gt0016",
        "priority": 4,
        "when": [
          "$gt0003|Diagnostic certainty|==local::at0076|Confirmed|",
          "($gt0015|Severity|==local::at0049|Severe|)||($gt0015|Severity|==local::at0079|Critical|)"
        ],
        "then": [
          "$gt0029|Recommendation|='Nasal catheter for oxygen or Face-masked Oxygen Supply'"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 3,
        "when": [
          "$gt0003|Diagnostic certainty|==local::at0076|Confirmed|",
          "($gt0015|Severity|==local::at0049|Severe|)||($gt0015|Severity|==local::at0079|Critical|)",
          "($gt0021|Problem/Diagnosis name|=='Hypoxemia')||($gt0021|Problem/Diagnosis name|=='Respiratory distress')",
          "$gt0024|Resolution phase|==local::at0086|Not resolving|"
        ],
        "then": [
          "$gt0026|Last updated|=$currentDateTime",
          "$gt0029|Recommendation|='High-flow nasal cannula (HFNC) oxygen therapy or Non-invasive mechanical ventilation'"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 2,
        "when": [
          "$gt0003|Diagnostic certainty|==local::at0076|Confirmed|",
          "($gt0015|Severity|==local::at0049|Severe|)||($gt0015|Severity|==local::at0079|Critical|)",
          "($gt0021|Problem/Diagnosis name|=='Hypoxemia')||($gt0021|Problem/Diagnosis name|=='Respiratory distress')",
          "$gt0027|Last updated|>($currentDateTime-2,h)",
          "$gt0027|Last updated|<=($currentDateTime-1,h)",
          "($gt0024|Resolution phase|==local::at0086|Not resolving|)||($gt0024|Resolution phase|==local::at0097|Relapsed|)"
        ],
        "then": [
          "$gt0009|Medication item|='Sedation and muscle relaxants'",
          "$gt0029|Recommendation|='Invasive mechanical ventilation'"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 1,
        "when": [
          "$gt0003|Diagnostic certainty|==local::at0076|Confirmed|",
          "($gt0015|Severity|==local::at0049|Severe|)||($gt0015|Severity|==local::at0079|Critical|)",
          "$gt0021|Problem/Diagnosis name|=='Acute Respiratory Distress Syndrome'",
          "$gt0024|Resolution phase|==local::at0097|Relapsed|"
        ],
        "then": [
          "$gt0029|Recommendation|='Lung recruitment,Prone ventilation,ECMO'"
        ]
      }
    }
  },
