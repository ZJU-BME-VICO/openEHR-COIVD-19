        }
      },
      "gt0047": {
        "id": "gt0047",
        "model_id": "openEHR-EHR-EVALUATION.recommendation.v1",
        "template_id": "openEHR-EHR-EVALUATION.recommendation.v1",
        "type": "OUTPUT",
        "elements": {
          "gt0048": {
            "id": "gt0048",
            "path": "/data[at0001]/items[at0002]"
          }
        }
      }
    },
    "rules": {
      "gt0028": {
        "id": "gt0028",
        "priority": 2,
        "when": [
          "$gt0019|Body Temperature Comment|=='Body temperature is normal for more than three consecutive days'",
          "$gt0025|Lab Test Conclusion|=='Continuous twice negative results on nucleic acid detection of respiratory specimen(the interval time of sampling is at least 1 day)'",
          "((($gt0033|Symptom/Sign name|=='Dry Cough')||($gt0033|Symptom/Sign name|=='Rhinorrhea'))||($gt0033|Symptom/Sign name|=='Nasal Congestion'))||(($gt0033|Symptom/Sign name|=='Pain in throat')||($gt0033|Symptom/Sign name|=='Difficulty breathing'))",
          "$gt0032|Progression|==local::at0184|Resolved|",
          "$gt0037|Diagnostic certainty|==local::at0076|Confirmed|",
          "$gt0049|Problem/Diagnosis name|=='COVID-19'",
          "$gt0050|Imaging result details|=='Substantial improvement of acute exudative lesions'"
        ],
        "then": [
          "$gt0048|Recommendation|='Discharge from hospital'"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 1,
        "when": [
          "!fired($gt0028)"
        ],
        "then": [
          "$gt0048|Recommendation|='Continue isolation treatment'"
        ]
      }
    }
  },
