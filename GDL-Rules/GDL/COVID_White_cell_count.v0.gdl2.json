    "rules": {
      "gt0008": {
        "id": "gt0008",
        "priority": 3,
        "when": [
          "$gt0003|Analyte name|=='White blood cell count(WBC)'",
          "$gt0004|Analyte result|<'3.8'"
        ],
        "then": [
          "$gt0007|Conclusion|='low white cell count'"
        ]
      },
      "gt0009": {
        "id": "gt0009",
        "priority": 2,
        "when": [
          "$gt0003|Analyte name|=='White blood cell count(WBC)'",
          "$gt0004|Analyte result|>='3.8'",
          "$gt0004|Analyte result|<'11'"
        ],
        "then": [
          "$gt0007|Conclusion|='normal white cell count'"
        ]
      },
      "gt0010": {
        "id": "gt0010",
        "priority": 1,
        "when": [
          "$gt0003|Analyte name|=='White blood cell count(WBC)'",
          "$gt0004|Analyte result|>='11'"
        ],
        "then": [
          "$gt0007|Conclusion|='high white cell count'"
        ]
      }
    }
