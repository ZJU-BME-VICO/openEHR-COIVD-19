        "template_id": "openEHR-EHR-OBSERVATION.imaging_exam_result.v0",
        "type": "INPUT",
        "elements": {
          "gt0036": {
            "id": "gt0036",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[openEHR-EHR-CLUSTER.imaging_result-COVID_19.v0]/items[at0.15]/items[at0.16]"
          }
        }
      },
      "gt0010": {
        "id": "gt0010",
        "model_id": "openEHR-EHR-OBSERVATION.story.v1",
        "template_id": "openEHR-EHR-OBSERVATION.story.v1",
        "type": "INPUT",
        "elements": {
          "gt0011": {
            "id": "gt0011",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[openEHR-EHR-CLUSTER.symptom_sign.v1]/items[at0001]"
          },
          "gt0033": {
            "id": "gt0033",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[openEHR-EHR-CLUSTER.symptom_sign.v1]/items[at0002]"
          }
        }
      },
      "gt0012": {
        "id": "gt0012",
        "model_id": "openEHR-EHR-OBSERVATION.laboratory_test_result.v1",
        "template_id": "openEHR-EHR-OBSERVATION.laboratory_test_result.v1",
        "type": "INPUT",
        "elements": {
          "gt0013": {
            "id": "gt0013",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[openEHR-EHR-CLUSTER.laboratory_test_analyte.v1]/items[at0024]"
          },
          "gt0014": {
            "id": "gt0014",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[openEHR-EHR-CLUSTER.laboratory_test_analyte.v1]/items[at0001]"
          }
        }
      },
      "gt0015": {
        "id": "gt0015",
        "model_id": "openEHR-EHR-OBSERVATION.pulse_oximetry.v1",
        "template_id": "openEHR-EHR-OBSERVATION.pulse_oximetry.v1",
        "type": "INPUT",
        "elements": {
          "gt0032": {
            "id": "gt0032",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0036]"
          }
        }
      },
      "gt0018": {
        "id": "gt0018",
        "model_id": "openEHR-EHR-OBSERVATION.respiration.v2",
        "template_id": "openEHR-EHR-OBSERVATION.respiration.v2",
        "type": "INPUT",
        "elements": {
          "gt0019": {
            "id": "gt0019",
            "path": "/data[at0001]/events[at0002]/state[at0022]/items[openEHR-EHR-CLUSTER.inspired_oxygen.v1]/items[at0051]"
          },
          "gt0020": {
            "id": "gt0020",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0004]"
          }
        }
      }
    },
    "rules": {
      "gt0024": {
        "id": "gt0024",
        "priority": 5,
        "when": [
          "$gt0013|Analyte name|=='PaO₂'",
          "$gt0019|Flow rate|.unit=='l/min'"
        ],
        "then": [
          "$gt0025|PaO2/FiO2|=$gt0014/(21+4*$gt0019.magnitude)*100"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 4,
        "when": [
          "$gt0003|Diagnostic certainty|==local::at0076|Confirmed|",
          "$gt0033|Symptom Description|=='slight clinical symptoms'",
          "$gt0036|Imaging result details|=='no imaging findings of COVID-19'"
        ],
        "then": [
          "$gt0006|Severity|=local::at0047|Mild|"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 3,
        "when": [
          "$gt0003|Diagnostic certainty|==local::at0076|Confirmed|",
          "($gt0011|Symptom/Sign name|=='Fever')||((($gt0011|Symptom/Sign name|=='Dry Cough')||($gt0011|Symptom/Sign name|=='Pain in throat'))||((($gt0011|Symptom/Sign name|=='Rhinorrhea')||($gt0011|Symptom/Sign name|=='Nasal Congestion'))||($gt0011|Symptom/Sign name|=='Difficulty breathing')))",
          "$gt0036|Imaging result details|=='With imaging findings of COVID-19'"
        ],
        "then": [
          "$gt0006|Severity|=local::at0048|Moderate|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 2,
        "when": [
          "$gt0003|Diagnostic certainty|==local::at0076|Confirmed|",
          "(($gt0020|Respiratoty Rate|>=30,/min)||($gt0032|Pulse oximetry Comment|=='Blood oxygen saturation of finger is less than or equal to 93% in the resting state'))||(($gt0025|PaO2/FiO2|<=300)||($gt0036|Imaging result details|=='Cases with chest imaging that showed obvious lesion progression within 24-48 hours >50%'))"
        ],
        "then": [
          "$gt0006|Severity|=local::at0049|Severe|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 1,
        "when": [
          "$gt0003|Diagnostic certainty|==local::at0076|Confirmed|",
          "($gt0030|Problem/Diagnosis name|=='Respiratory failure ')||(($gt0030|Problem/Diagnosis name|=='Shock')||($gt0030|Problem/Diagnosis name|=='Multiple organ failure '))"
        ],
        "then": [
          "$gt0006|Severity|=local::at0079|Critical|"
        ]
      }
    }
  },
